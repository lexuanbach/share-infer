BIABDUCTION
REC tree(VAR x):=(VAR x = NULL) OR (EXT (VAR x1) (EXT (VAR x2) (EXT (VAR d) (((VAR x) |--B--> (VAR d,VAR x1,VAR x2)) * ((tree(VAR x1)) * (tree(VAR x2)))))))
((VAR a) |--[ B W ]--> (VAR b,VAR c,VAR d)) * ((VAR d) |--[ W [ W B ] ]--> (VAR d1,VAR e,VAR f))
(([ B W ]) <.> (tree(VAR a))) * (([ W B ]) <.> (tree(VAR d)))
